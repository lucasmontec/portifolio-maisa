<ul>
{% for item in menu %}
    <li>
        {% if item|current %}
            <a class="selected" id="{{item.label}}" href="{{ item.link }}">{{item.label}}</a>
        {% else %}
            <a id="{{item.label}}" href="{{ item.link }}">{{item.label}}</a>
        {% endif %} 
    </li>
{% endfor %}
</ul>